{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置  // [!code ++]
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^Wec(.*)": "@/components/Wec$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTextStyle": "white",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/community/community",
      "style": {
        "navigationBarTitleText": "社区"
      }
    },
    {
      "path": "pages/health/health",
      "style": {
        "navigationBarTitleText": "健康监测"
      }
    },
    {
      "path": "pages/service/service",
      "style": {
        "navigationBarTitleText": "服务"
      }
    },
    {
      "path": "pages/profile/profile",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#333",
    "selectedColor": "#27ba9b",
    "backgroundColor": "#fff",
    "borderStyle": "white",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/images/tabbar/home.png",
        "selectedIconPath": "static/images/tabbar/home_active.png"
      },
      {
        "pagePath": "pages/community/community",
        "text": "社区",
        "iconPath": "static/images/tabbar/community.png",
        "selectedIconPath": "static/images/tabbar/community_active.png"
      },
      {
        "pagePath": "pages/health/health",
        "text": "健康监测",
        "iconPath": "static/images/tabbar/health_monitoring.png",
        "selectedIconPath": "static/images/tabbar/health_monitoring_active.png"
      },
      {
        "pagePath": "pages/service/service",
        "text": "服务",
        "iconPath": "static/images/tabbar/service.png",
        "selectedIconPath": "static/images/tabbar/service_active.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "text": "我的",
        "iconPath": "static/images/tabbar/profile.png",
        "selectedIconPath": "static/images/tabbar/profile_active.png"
      }
    ]
  }
}
